<?xml version='1.0' encoding='utf-8'?>
<widget id="es.okmenu" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>okmenu</name>
    <description>
        Genera tu menu diario
    </description>
    <author email="ojkare@gmail.com" href="http://www.okmenu.es">
        okmenu
    </author>
    <content src="index.html" />
	<!--preference name='phonegap-version' value='cli-7.1.0' /-->
	<preference  name="phonegap-version" value="cli-8.1.1" />  
	<preference name="DisallowOverscroll" value="true" />

	<icon src="www/icon.png"/>
	<!--plugin name="phonegap-plugin-push"					source="npm" spec="2.2.3"/-->
	<plugin name="cordova-plugin-statusbar"				source="npm" spec="2.4.2" />
	<plugin name="cordova-plugin-device"				source="npm" spec="2.0.2" />
	<plugin name="cordova-plugin-media" 				source="npm" />
	<plugin name="cordova-plugin-camera"				source="npm" spec="4.0.3" />
	<plugin name="cordova-plugin-console"				source="npm" spec="1.1.0" />
	<!--plugin name="cordova-plugin-geolocation"			source="npm" spec="4.0.1" /-->
	<plugin name="cordova-plugin-inappbrowser"			source="npm" spec="3.0.0" />
	<plugin name="cordova-plugin-network-information"	source="npm" spec="2.0.1" />
	<plugin name="cordova-plugin-whitelist"				source="npm" spec="1.3.3" />
	<plugin name="cordova-plugin-media-capture"			source="npm" spec="3.0.2" />
	<plugin name="cordova-plugin-file-transfer"			source="npm" spec="1.7.1" />
	<plugin name="cordova-plugin-file-opener2"			source="npm" spec="^2.1.3" >
		 <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
	</plugin>
	<plugin name="phonegap-plugin-barcodescanner"		source="npm" spec="8.0.1">
		<variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
	</plugin>

	<preference name="StatusBarOverlaysWebView" value="true" />
	<preference name="StatusBarBackgroundColor" value="#4d3d3d" />
	<preference name="StatusBarStyle" value="lightcontent" />

	<platform name="android">
		<!--resource-file src="www/google-services.json" target="app/google-services.json" /-->
		<plugin name="cordova-plugin-file"					source="npm"/>
		<preference name="AndroidPersistentFileLocation" value="Internal" />
		<preference name="AndroidPersistentFileLocation" value="Compatibility" />
		<!--icon	platform="android" qualifier="ldpi"			src="www/res/icons/android/icon-36-ldpi.png" />
		<icon	platform="android" qualifier="mdpi"			src="www/res/icons/android/icon-48-mdpi.png" />
		<icon	platform="android" qualifier="hdpi"			src="www/res/icons/android/icon-72-hdpi.png" />
		<icon	platform="android" qualifier="xhdpi"		src="www/res/icons/android/icon-96-xhdpi.png" />
		<icon	platform="android" qualifier="xxhdpi"		src="www/res/icons/android/icon-144-xxhdpi.png" />
		<icon	platform="android" qualifier="xxxhdpi"		src="www/res/icons/android/icon-192-xxxhdpi.png" /-->
		<splash platform="android" qualifier="land-hdpi"	src="www/res/screens/android/screen-hdpi-landscape.png" />
		<splash platform="android" qualifier="land-ldpi"	src="www/res/screens/android/screen-ldpi-landscape.png" />
		<splash platform="android" qualifier="land-mdpi"	src="www/res/screens/android/screen-mdpi-landscape.png" />
		<splash platform="android" qualifier="land-xhdpi"	src="www/res/screens/android/screen-xhdpi-landscape.png" />
		<splash platform="android" qualifier="port-hdpi"	src="www/res/screens/android/screen-hdpi-portrait.png" />
		<splash platform="android" qualifier="port-ldpi"	src="www/res/screens/android/screen-ldpi-portrait.png" />
		<splash platform="android" qualifier="port-mdpi"	src="www/res/screens/android/screen-mdpi-portrait.png" />
		<splash platform="android" qualifier="port-xhdpi"	src="www/res/screens/android/screen-xhdpi-portrait.png" />
    </platform>
	<platform name="ios">
		<!--resource-file src="www/GoogleService-Info.plist" target="app/GoogleService-Info.plist" /-->
		<preference name="iosPersistentFileLocation" value="Library" />
		<preference name="iosPersistentFileLocation" value="Compatibility" />
		<preference name="BackupWebStorage" value="none" />
		<icon platform="ios" height="60"		width="60"		src="www/res/icons/ios/icon-60.png" />
		<icon	platform="ios" height="120"		width="120"		src="www/res/icons/ios/icon-60-2x.png" />
		<icon	platform="ios" height="180"		width="180"		src="www/res/icons/ios/icon-60-3x.png" />
		<icon	platform="ios" height="76"		width="76"		src="www/res/icons/ios/icon-76.png" />
		<icon	platform="ios" height="152"		width="152"		src="www/res/icons/ios/icon-76-2x.png" />
		<icon	platform="ios" height="40"		width="40"		src="www/res/icons/ios/icon-40.png" />
		<icon	platform="ios" height="57"		width="57"		src="www/res/icons/ios/icon-57.png" />
		<icon	platform="ios" height="114"		width="114"		src="www/res/icons/ios/icon-57-2x.png" />
		<icon	platform="ios" height="72"		width="72"		src="www/res/icons/ios/icon-72.png" />
		<icon	platform="ios" height="144"		width="144"		src="www/res/icons/ios/icon-72-2x.png" />
		<icon	platform="ios" height="29"		width="29"		src="www/res/icons/ios/icon-small.png" />
		<icon	platform="ios" height="58"		width="58"		src="www/res/icons/ios/icon-small-2x.png" />
		<icon	platform="ios" height="50"		width="50"		src="www/res/icons/ios/icon-50.png" />
		<icon	platform="ios" height="100"		width="100"		src="www/res/icons/ios/icon-50-2x.png" />
		<splash platform="ios" height="480"		width="320"		src="www/res/screens/ios/screen-iphone-portrait.png" />
		<splash platform="ios" height="960"		width="640"		src="www/res/screens/ios/screen-iphone-portrait-2x.png" />
		<splash platform="ios" height="1024"	width="768"		src="www/res/screens/ios/screen-ipad-portrait.png" />
		<splash platform="ios" height="2048"	width="1536"	src="www/res/screens/ios/screen-ipad-portrait-2x.png" />
		<splash platform="ios" height="768"		width="1024"	src="www/res/screens/ios/screen-ipad-landscape.png" />
		<splash platform="ios" height="1536"	width="2048"	src="www/res/screens/ios/screen-ipad-landscape-2x.png" />
		<splash platform="ios" height="1136"	width="640"		src="www/res/screens/ios/screen-iphone-568h-2x.png" />
		<splash platform="ios" height="1334"	width="750"		src="www/res/screens/ios/screen-iphone-portrait-667h.png" />
		<splash platform="ios" height="2208"	width="1242"	src="www/res/screens/ios/screen-iphone-portrait-736h.png" />
		<splash platform="ios" height="1242"	width="2208"	src="www/res/screens/ios/screen-iphone-landscape-736h.png" />
	</platform>

 
	<access origin="*" />
	<allow-navigation href="*" />
    <allow-intent href="*" />
	<allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
